   %N-shlibs (= %v-%r), glib2-shlibs, libiconv, gettext
<<
BuildDepends: <<
   glib2-dev, gtk-doc, libiconv-bin, libiconv-dev, gettext-dev
<<
BuildDependsOnly: True
