+++ auctex-11.82.patched/fink/install	2005-12-19 14:04:31.000000000 -0500
@@ -0,0 +1,138 @@
+#! /bin/bash -e
+#
+# <% elisp %> emacs install script for auctex
+#
+# Copyright (C) 1997, 98, 99, 2000, 01, 02, 03, 04, 05
+# by Davide G. M. Salvetti for Debian version
+#
+# Copyright (C) 2006 Sebastien Maret for Fink version
+#
+# This program is free software; you can redistribute it and/or modify it
+# under the terms of the GNU General Public License as published by the
+# Free Software Foundation; either version 2 of the License, or (at your
+# option) any later version.
+#
+# This program is distributed in the hope that it will be useful, but
+# WITHOUT ANY WARRANTY; without even the implied warranty of
+# MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE.  See the GNU
+# General Public License for more details.
+#
+# You should have received a copy of the GNU General Public License along
+# with this program; if not, write to: The Free Software Foundation, Inc.,
+# 51 Franklin St, Fifth Floor, Boston, MA 02110-1301, USA.
+#
+# On Debian GNU/Linux System you can find a copy of the GNU General Public
+# License in "/usr/share/common-licenses/GPL".
+
+set -o posix
+
+FLAVOR=${1}
+
+INITSRC="auctex.el preview-latex.el tex-site.el"
+
+AUCTEXSRC="tex.el tex-buf.el tex-style.el latex.el tex-info.el \
+    texmathp.el multi-prompt.el tex-mik.el font-latex.el tex-font.el \
+    context.el context-en.el context-nl.el tex-fold.el \
+    toolbar-x.el tex-bar.el bib-cite.el tex-fptex.el"
+
+STYLESRC="style/prosper.el \
+    style/slides.el    style/foils.el     style/amstex.el \
+    style/article.el   style/book.el      style/letter.el \
+    style/report.el    style/amsart.el    style/amsbook.el \
+    style/epsf.el      style/psfig.el     style/latexinfo.el \
+    style/dutch.el     style/german.el    style/dk.el \
+    style/j-article.el style/j-book.el    style/j-report.el \
+    style/jarticle.el  style/jbook.el     style/jreport.el \
+    style/dinbrief.el  style/virtex.el    style/plfonts.el \
+    style/plhb.el      style/harvard.el	  style/swedish.el \
+    style/danish.el    style/slovak.el    style/czech.el \
+    style/amsmath.el   style/amstext.el   style/amsbsy.el \
+    style/amsopn.el    style/amsthm.el	  style/natbib.el \
+    style/index.el     style/makeidx.el   style/multind.el \
+    style/varioref.el  style/fancyref.el  style/mdwlist.el \
+    style/ngerman.el   style/graphicx.el  style/graphics.el \
+    style/verbatim.el  style/scrbase.el   style/scrartcl.el \
+    style/scrbook.el   style/scrreprt.el  style/scrlttr2.el \
+    style/scrpage2.el  style/captcont.el  style/subfigure.el \
+    style/paralist.el  style/booktabs.el  style/emp.el \
+    style/jura.el      style/alphanum.el  style/beamer.el \
+    style/alltt.el     style/italian.el   style/doc.el \
+    style/ltxdoc.el    style/ltx-base.el  style/units.el \
+    style/nicefrac.el  style/url.el       style/listings.el \
+    style/jurabib.el   style/csquotes.el  style/jsarticle.el \
+    style/jsbook.el    style/babel.el     style/dk-bib.el \
+    style/inputenc.el  style/frenchb.el   style/francais.el \
+    style/MinionPro.el"
+
+PREVIEWSRC="preview.el  prv-emacs.el"
+
+ELCC="${FLAVOR} -batch -q -no-site-file -no-init-file -l lpath.el"
+
+do_install () {
+    echo >&2 -n "auctex/install: Setting up for ${FLAVOR}... "
+    install -d @PREFIX@/share/${FLAVOR}/site-lisp
+    install -d @PREFIX@/share/${FLAVOR}/site-lisp/auctex
+    install -d @PREFIX@/share/${FLAVOR}/site-lisp/auctex/style
+    ln -s @PREFIX@/share/emacs/site-lisp/auctex/images \
+	@PREFIX@/share/${FLAVOR}/site-lisp/auctex \
+	>> @PREFIX@/share/${FLAVOR}/site-lisp/auctex//CompilationLog 2>&1
+    install -m 644 @PREFIX@/share/emacs/site-lisp/auctex.el \
+	@PREFIX@/share/emacs/site-lisp/preview-latex.el \
+	@PREFIX@/share/emacs/site-lisp/tex-site.el \
+	@PREFIX@/share/${FLAVOR}/site-lisp \
+	>> @PREFIX@/share/${FLAVOR}/site-lisp/auctex//CompilationLog 2>&1
+    install -m 644 @PREFIX@/share/emacs/site-lisp/auctex/lpath.el \
+	@PREFIX@/share/${FLAVOR}/site-lisp/auctex \
+	>> @PREFIX@/share/${FLAVOR}/site-lisp/auctex//CompilationLog 2>&1
+    (umask 022 && \
+	touch @PREFIX@/share/${FLAVOR}/site-lisp/auctex/style/.nosearch)
+    for i in ${STYLESRC}
+    do install -m 644 @PREFIX@/share/emacs/site-lisp/auctex/${i} \
+	@PREFIX@/share/${FLAVOR}/site-lisp/auctex/style
+    done
+    for i in ${AUCTEXSRC} ${PREVIEWSRC}
+    do install -m 644 @PREFIX@/share/emacs/site-lisp/auctex/${i} \
+	@PREFIX@/share/${FLAVOR}/site-lisp/auctex
+    done
+    cd @PREFIX@/share/${FLAVOR}/site-lisp
+#    ${ELCC} -f batch-byte-compile ${INITSRC} \
+#	>> @PREFIX@/share/${FLAVOR}/site-lisp/auctex//CompilationLog 2>&1
+    cd @PREFIX@/share/${FLAVOR}/site-lisp/auctex
+    ${ELCC} -f batch-byte-compile ${AUCTEXSRC} ${PREVIEWSRC} ${STYLESRC} \
+	>> @PREFIX@/share/${FLAVOR}/site-lisp/auctex//CompilationLog 2>&1
+#    for i in ${INITSRC}
+#    do rm -f @PREFIX@/share/${FLAVOR}/site-lisp/${i} \
+#	>> @PREFIX@/share/${FLAVOR}/site-lisp/auctex//CompilationLog 2>&1
+#    done
+    find @PREFIX@/share/${FLAVOR}/site-lisp/auctex/ -type f -name \*.el \
+	-print0 | xargs --null rm -f ,dummy, \
+	>> @PREFIX@/share/${FLAVOR}/site-lisp/auctex//CompilationLog 2>&1
+    rm -f @PREFIX@/share/${FLAVOR}/site-lisp/auctex/lpath.el \
+	@PREFIX@/share/${FLAVOR}/site-lisp/preview/lpath.el \
+	>> @PREFIX@/share/${FLAVOR}/site-lisp/auctex//CompilationLog 2>&1
+    gzip -9fq @PREFIX@/share/${FLAVOR}/site-lisp/auctex//CompilationLog
+    echo >&2 "done."
+    return 0
+}
+
+do_auto () {
+    LOGFILE="@PREFIX@/var/auctex/UpdateLog"
+    echo >&2 -n "update-auctex-elisp: "
+    echo >&2 "Further output will appear in: ${LOGFILE}.gz."
+    echo >&2 -n "auctex: "
+    echo >&2 -n "Waiting for update-auctex-elisp to terminate... "
+    @PREFIX@/sbin/update-auctex-elisp ${FLAVOR} > ${LOGFILE} 2>&1
+    gzip -9fq ${LOGFILE}
+    echo >&2 "done."
+    return 0
+}
+
+case "${FLAVOR}" in
+    (emacs) : ;;
+    (emacs21|emacs22)
+    do_install ${FLAVOR}
+    do_auto ${FLAVOR} ;;
+    (*) echo >&2 "auctex/install: Ignoring emacsen flavor ${FLAVOR}." ;;
+esac
+
+exit 0
