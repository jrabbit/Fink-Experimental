+#
+# On Debian GNU/Linux System you can find a copy of the GNU General Public
+# License in "/usr/share/common-licenses/GPL".
+
+set -o posix
+
+FLAVOR=${1}
+
+undo_install () {
+    echo >&2 -n "auctex/remove: Removing for ${FLAVOR}... "
+    rm -f @PREFIX@/share/${FLAVOR}/site-lisp/auctex//../tex-site.el
+    rm -f @PREFIX@/share/${FLAVOR}/site-lisp/auctex//../auctex.el
+    rm -f @PREFIX@/share/${FLAVOR}/site-lisp/auctex//../preview-latex.el
+    rm -f @PREFIX@/share/${FLAVOR}/site-lisp/auctex//CompilationLog{,.gz}
+    rm -f @PREFIX@/share/${FLAVOR}/site-lisp/auctex//*.elc
+    rm -f @PREFIX@/share/${FLAVOR}/site-lisp/auctex//style//*.elc
+    rm -f @PREFIX@/share/${FLAVOR}/site-lisp/auctex//style//.nosearch
+    rm -f @PREFIX@/share/${FLAVOR}/site-lisp/auctex/images
+    rm -f @PREFIX@/share/${FLAVOR}/site-lisp/preview//*.elc
+    test -d @PREFIX@/share/${FLAVOR}/site-lisp/auctex//style/ && \
+	rmdir @PREFIX@/share/${FLAVOR}/site-lisp/auctex//style/ || true
+    test -d @PREFIX@/share/${FLAVOR}/site-lisp/auctex/ && \
+	rmdir @PREFIX@/share/${FLAVOR}/site-lisp/auctex/ || true
+    test -d @PREFIX@/share/${FLAVOR}/site-lisp/preview/ && \
+	rmdir @PREFIX@/share/${FLAVOR}/site-lisp/preview/ || true
+    echo >&2 "done."
+    return 0
+}
+
+undo_auto () {
+    rm -rf @PREFIX@/var/lib/auctex//${FLAVOR}/
+    return 0
+}
+
+case "${FLAVOR}" in
+    (emacs) : ;;
+    (emacs21|emacs22)
+        undo_auto
+        undo_install
+    ;;
+    (*) echo >&2 "auctex/remove:" \
+	"Ignoring emacsen flavor: \"${FLAVOR}\""
+    ;;
+esac
+
+exit 0
diff -ruN -x '*~' auctex-11.82/fink/update-auctex-elisp auctex-11.82.patched/fink/update-auctex-elisp
