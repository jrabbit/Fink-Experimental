#!/bin/sh 

if [ "$1" = "-V" ]; then
exec gcc $*
else
exec gcc -I@PREFIX@/lib/fontconfig2/include/ -L@PREFIX@/lib/fontconfig2/lib -I@PREFIX@/lib/freetype219/include -L@PREFIX@/lib/freetype219/lib/ @PREFIX@/lib/freetype219/lib/libfreetype.a -lz -Wl,-search_paths_first -Wl,-dylib_file,@PREFIX@/lib/libpango-1.0.0.dylib:@PREFIX@/lib/pango-ft219/lib/libpango-1.0.0.dylib -Wl,-dylib_file,@PREFIX@/lib/libpangox-1.0.0.dylib:@PREFIX@/lib/pango-ft219/lib/libpangox-1.0.0.dylib -Wl,-dylib_file,@PREFIX@/lib/libpangoxft-1.0.0.dylib:@PREFIX@/lib/pango-ft219/lib/libpangoxft-1.0.0.dylib -Wl,-dylib_file,@PREFIX@/lib/libpangoft2-1.0.0.dylib:@PREFIX@/lib/pango-ft219/lib/libpangoft2-1.0.0.dylib $@
fi