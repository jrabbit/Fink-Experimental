BuildDepends: nautilus-dev, gnome-panel-dev, libgtkhtml2, libzvt2-dev, pygtk2-py23-dev, libpng3, gal21, gdk-pixbuf, libjpeg, libtiff, netpbm, audiofile, db3, esound, glib2, gtk+2-dev, orbit-dev, readline, giflib, imlib, gdbm, gmp, libbonobo2-dev, gconf2-dev, glibwww, libgnome2-dev, gtkhtml1.1-dev, oaf-dev, expat, libwww, libxml, popt, tcltk-dev, libglade, gnome-vfs2-dev | gnome-vfs2-ssl-dev, gettext-dev, gettext-bin, libiconv-dev
#Replaces: gnome-python, gnome-python-py22
#Conflicts: gnome-python-py22
Provides: gnome-python2
Patch: %n.patch
SetCPPFLAGS: -no-cpp-precomp
#SetCFLAGS: -I%p/include/libcapplet1 -I%p/include/gdk-pixbuf-1.0
ConfigureParams: --with-gtkhtml
CompileScript: <<
export PYTHON='%p/bin/python2.3'; ./configure %c
make
<<
InstallScript: <<
 make install prefix=%i
 install -d -m 755 %i/share/doc/%n/examples
 install -m 644 pygnome/examples/* %i/share/doc/%n/examples
<<
DocFiles: COPYING NEWS README
