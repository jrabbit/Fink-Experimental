Depends: gtk+2
BuildDepends: glib2-dev, atk1, pango1-dev, gtk+2-dev (>= 2.0.5-2), libgnomecanvas2-dev, linc1, orbit2-dev, gconf2-dev, gnome-vfs2-dev (>= 2.4.0-1) | gnome-vfs2-ssl-dev (>= 2.4.0-1), libxml2, libbonobo2-dev, libgnome2-dev (>= 2.0.6-2), libbonoboui2-dev (>= 2.0.3-3), libgnomeui2-dev (>= 2.0.6-2), pkgconfig, intltool, popt, gettext-dev, gettext-bin, libiconv-dev
Source: mirror:gnome:sources/%n/0.10/%n-%v.tar.bz2
Source-MD5: bc7fc686f91c5ca848d4ae86f58c76f9
SetCPPFLAGS: -no-cpp-precomp
SetCFLAGS: -O3 -funroll-loops -fstrict-aliasing
ConfigureParams: --libexecdir=%p/lib/%n --mandir=%p/share/man
NoSetMAKEFLAGS: true
SetMAKEFLAGS: -j1
CompileScript: <<
  F77=no ./configure %c
  make
<<
InstallScript: <<
  GCONF_DISABLE_MAKEFILE_SCHEMA_INSTALL=1 make install DESTDIR=%d
<<
