+#		include <stdio.h>
+		void sub(p1,p2) char *p1, *p2;
+		    { (void)printf("%ld\n", (long)(p2-p1)); }
+		void sub_(p1,p2) char *p1,*p2; { sub(p1,p2); }
+		void SUB(p1,p2) char *p1,*p2; { sub(p1,p2); }
