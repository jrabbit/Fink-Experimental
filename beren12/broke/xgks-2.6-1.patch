+dnl Convert a pathname to an absolute one at autoconf(1) execution time.
+dnl
+AC_DEFUN([UC_ABSPATH_M4], [dnl
+syscmd([case "$1" in 
+  /*)
+	echo $1; exit;;
+   *)
+	path=`pwd`/$1
+	tail=
+	while test "$path"; do
+	    (cd $path && echo `pwd`$rest) && exit
+	    base=/`basename "$path"`
+	    tail=/$base$tail
+	    path=`echo "$path" | sed "s/\/$base//"`
+	done;;
+esac > conftest.syscmd 2>&1
+])dnl
+include(conftest.syscmd)dnl
+])
