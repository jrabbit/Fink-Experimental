#echo "WARNING: if you compile X11 applications against this X.org release,"
#echo "you will *not* be able to run them if you decide to revert to Apple's"
#echo "X11 provided with the OS."

case "$1" in
	install)
		echo "If you have previously build packages against XFree86 > 4.4 or 
		echo "X.org, they will very likely no longer run."
		;;
esac
<<
PostInstScript: <<
# Based on the X11User postflight
/usr/X11R6/bin/fc-cache /System/Library/Fonts /Library/Fonts
<<
PreRmScript: <<
case "$1" in
	remove|purge)
		find /usr/X11R6 -name 'fonts.cache*' -exec rm -rf {} \;
		;;
esac
