+
+    lbl = new QLabel(i18n("&Comment:"), frame);
+    grid->addWidget(lbl, 1, 0);
+    m_CommentEdit = new QLineEdit(frame);
+    lbl->setBuddy(m_CommentEdit);
+    grid->addWidget(m_CommentEdit, 1, 1);
