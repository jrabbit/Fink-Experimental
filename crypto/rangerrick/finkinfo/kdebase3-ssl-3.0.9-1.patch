+void KBackground::slotSetupMulti()
+{
+    KBackgroundRenderer *r = m_Renderer;
+
+    KMultiWallpaperDialog dlg(r);
+    if (dlg.exec() == QDialog::Accepted) {
+    r->stop();
+    r->start();
+    emit changed(true);
+    }
+}
