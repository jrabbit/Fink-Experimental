Package: evolution-webcal
Version: 2.6.0
Revision: 1
Source-MD5: c779dcfc1db419f019abfcaafd6b8e4d
Source: mirror:gnome:sources/%n/2.6/%n-%v.tar.bz2
Depends: evolution2
BuildDepends: atk1, evolution-data-server1-dev, evolution2-dev, gconf2-dev, glib2-dev, gnome-keyring-dev, gnome-vfs2-unified-dev, gnutls12, gtk+2-dev, libart2, libbonobo2-dev, libbonoboui2-dev, libgcrypt, libgettext3-dev, libgnome2-dev, libgnomecanvas2-dev, libgnomeui2-dev, libgpg-error, libiconv-dev, libsoup2.2.8-ssl, libtasn1, libxml2, orbit2-dev, pango1-xft2-dev, popt, x11-dev, pkgconfig, intltool, gettext-tools
NoSetLDFLAGS: true
SetLIBRARY_PATH: %p/lib
NoSetCPPFLAGS: true
SetCPPFLAGS: -no-cpp-precomp
SetCFLAGS: -O3 -fstrict-aliasing
PatchScript: <<
#!/bin/sh -ev
# cf e-d-s for next :
  for f in `find . -name ltmain.sh` ; do sed -ri.bak -e 's,(s%%/)(\[\^/\]\*\$%%%%),\1\\+\2,' $f ; done
  for f in `find . -name configure` ; do sed -ri.bak -e '/(host_os ld.so|Apple)/,+3{
/sys_lib_search_path_spec.*\/usr\/local\/lib/c\
sys_lib_search_path_spec=$(echo -n $(for f in `$CC -print-search-dirs|egrep "^libraries:"|sed -e "s,.*=,," -e "s,$PATH_SEPARATOR, ,g" -e "s,$, /lib /usr/lib /usr/local/lib,"`; do cd $f 2>/dev/null && pwd || : ; done|uniq))
}' $f ; done
<<
ConfigureParams: --libexecdir=%p/sbin
CompileScript: <<
#!/bin/sh -ev
  export CPATH=%p/include
  ./configure %c
  perl -pi -e 's,\-L(/usr|%p)/lib([^/]),\2,g; s,\-I(/usr|%p)/include([^/]),\2,g' config.status
  make
  make -i check
<<
InstallScript: make DESTDIR=%d install
PostInstScript: <<       
  if [ configure = "$1" ]; then
    GCONF_CONFIG_SOURCE=`gconftool-2 --get-default-source` gconftool-2 --makefile-install-rule \
	%p/etc/gconf/schemas/evolution-webcal.schemas >/dev/null
  fi
<<
PreRmScript: <<
  if [ purge = "$1" ]; then
    GCONF_CONFIG_SOURCE=`gconftool-2 --get-default-source` gconftool-2 --makefile-uninstall-rule \
	%p/etc/gconf/schemas/evolution-webcal.schemas >/dev/null
  fi
<<
ConfFiles: %p/etc/gconf/schemas/evolution-webcal.schemas
