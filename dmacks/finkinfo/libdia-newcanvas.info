  (cd /usr/local/fink/libdia-newcanvas/bob/src; /bin/sh ../libtool --mode=relink gcc -g -O2 -Werror -g -L/sw/lib -o libgladedianewcanvas2.la -rpath /sw/lib/libglade/2.0 libglade.lo libdianewcanvas2.la)
