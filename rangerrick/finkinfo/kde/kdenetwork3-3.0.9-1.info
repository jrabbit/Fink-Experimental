BuildDepends: arts-dev (>= 1.1.0-5), kdelibs3-ssl-dev (>= %v-1) | kdelibs3-dev (>= %v-1), kdebase3-ssl-dev (>= %v-1) | kdebase3-dev (>= %v-1), automake (>= 1.6), autoconf25, libtool14, fink (>= 0.10.0), dlcompat (>= 20020929), libjpeg (>= 6b), libpng3 (>= 1.2.4), libpoll (>= 1.1), qt3 (>= 3.1.0)
