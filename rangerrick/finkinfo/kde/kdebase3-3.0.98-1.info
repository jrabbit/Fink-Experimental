<<
InstallScript: <<
#!/bin/sh
 make install DESTDIR=%d
 install -d -m 755 %i/share/apps/ksplash/pics
 install -c -m 644 ../kde-x/*.png %i/share/apps/ksplash/pics/

 # get rid of some stragglers
 for size in 16x16 22x22 32x32 48x48 64x64; do
  rm -rf %i/share/icons/crystalsvg/${size}/actions/view_choose.png
  rm -rf %i/share/icons/crystalsvg/${size}/actions/view_detailed.png
  rm -rf %i/share/icons/crystalsvg/${size}/actions/view_icon.png
  rm -rf %i/share/icons/crystalsvg/${size}/actions/view_multicolumn.png
  rm -rf %i/share/icons/crystalsvg/${size}/actions/view_text.png
  rm -rf %i/share/icons/crystalsvg/${size}/actions/view_tree.png
  rm -rf %i/share/icons/crystalsvg/${size}/devices/print_class.png
 done
<<
