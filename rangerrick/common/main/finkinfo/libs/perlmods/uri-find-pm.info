Package: uri-find-pm%type_pkg[perl]
Version: 0.16
Revision: 2
Depends: perl%type_pkg[perl]-core, uri-pm%type_pkg[perl]
BuildDepends: uri-pm%type_pkg[perl], fink (>= 0.20.2.1-1), perl%type_pkg[perl]-core
