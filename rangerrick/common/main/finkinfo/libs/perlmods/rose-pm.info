Package: rose-pm%type_pkg[perl]
Version: 0.05
Revision: 2
Description: A complete web application construction kit
Depends: perl%type_pkg[perl]-core, fink (>= 0.20.1-1), rose-object-pm%type_pkg[perl], rose-datetime-pm%type_pkg[perl], rose-html-objects-pm%type_pkg[perl], rose-conf-pm%type_pkg[perl], rose-db-pm%type_pkg[perl], rose-db-object-pm%type_pkg[perl], test-simple-pm%type_pkg[perl]
BuildDepends: perl%type_pkg[perl]-core, fink (>= 0.20.2.1-1), rose-object-pm%type_pkg[perl], rose-datetime-pm%type_pkg[perl], rose-html-objects-pm%type_pkg[perl], rose-conf-pm%type_pkg[perl], rose-db-pm%type_pkg[perl], rose-db-object-pm%type_pkg[perl], test-simple-pm%type_pkg[perl]

