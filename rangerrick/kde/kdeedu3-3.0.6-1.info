Package: kdeedu3
Version: 3.0.6
Revision: 1
Depends: flashkard (>= %v-%r), kalzium (>= %v-%r), keduca (>= %v-%r), kgeo (>= %v-%r), khangman (>= %v-%r), kiten (>= %v-%r), klettres (>= %v-%r), kmathtool (>= %v-%r), kmessedwords (>= %v-%r), kmplot (>= %v-%r), knorskverbs (>= %v-%r), kpercentage (>= %v-%r), kstars (>= %v-%r), ktouch (>= %v-%r), kverbos (>= %v-%r), kvoctrain (>= %v-%r)
BuildDepends: arts-dev (>= 1.0.6-1), kdelibs3-ssl-dev (>= 3.0.6-1) | kdelibs3-dev (>= 3.0.6-1), kdebase3-ssl-dev (>= 3.0.6-1) | kdebase3-dev (>= 3.0.6-1), automake, autoconf25, libtool14, fink (>= 0.9.12), dlcompat, libjpeg, libpng, qt3
