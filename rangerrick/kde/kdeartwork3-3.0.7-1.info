Package: kdeartwork3
Version: 3.0.7
Revision: 1
Depends: kde-extra-screensavers (>= %v-%r), kde-extra-sounds (>= %v-%r), kde-extra-themes (>= %v-%r), kde-extra-wallpapers (>= %v-%r), kde-icons-crystal (>= %v-%r), kde-icons-ikons (>= %v-%r), kde-icons-locolor (>= %v-%r), kde-icons-technical (>= %v-%r), kworldclock-maps (>= %v-%r)
BuildDepends: arts-dev (>= 1.0.7-1), kdelibs3-ssl-dev (>= %v-1) | kdelibs3-dev (>= %v-1), kdebase3-ssl-dev (>= %v-1) | kdebase3-dev (>= %v-1), automake (>= 1.6), autoconf25, libtool14, fink (>= 0.9.12), dlcompat, qt3, libpng, kworldclock (>= %v-1)
