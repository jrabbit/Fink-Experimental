PatchScript: sed 's|@PREFIX@|%p|g' <%a/%f.patch | patch -p0
SetLDFLAGS: -L%p/lib
SetCPPFLAGS: -no-cpp-precomp
SetCFLAGS: -fno-common
ConfigureParams: --prefix=%p --with-qt-dir=%p --with-extra-libs=%p/lib --with-extra-includes=%p/include --enable-mt --with-pic --enable-rpath --enable-shared=yes --enable-static=yes --mandir=%i/share/man --with-xinerama --enable-ncurses --enable-gtk --with-lame=%p --with-vorbis=%p --with-esd-prefix=%p --with-glib-prefix=%p --with-gtk-prefix=%p --with-xine-prefix=%p --enable-final $KDEOPTS
CompileScript: <<
 make -f Makefile.cvs
 sh configure %c
 (cd xine_artsplugin; make xinePlayObject.cc)
 make
<<
InstallScript: <<
 make install DESTDIR=%d
 rm -rf %i/share/doc/HTML/en/kscd
<<
DocFiles: AUTHORS COPYING ChangeLog INSTALL README
Description: KDE - Multimedia
DescDetail: <<
  Extra multimedia applications for KDE.
<<
License: GPL/LGPL
