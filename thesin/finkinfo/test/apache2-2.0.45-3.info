Package: apache2
Version: 2.0.45
Revision: 3
###
Conflicts: apache, apr, apache2
Replaces: apache, apr, apache2
Provides: httpd
BuildDepends: gawk
Depends: apache2-nompm | apache2-mpm-worker | apache2-mpm-perchild | apache2-mpm-prefork | apache2-mpm-leader | apache2-mpm-threadpool, apache2-common
###
CustomMirror: <<
  Primary: http://www.apache.org/dist/httpd/
  Secondary: http://www.apache.org/dist/httpd/old/
<<
###
Source: mirror:custom:httpd-%v.tar.gz
Source-MD5: 1f33e9a2e2de06da190230fa72738d75
###
PatchScript: sed -e 's,@FINKPREFIX@,%p,g' %a/%f.patch | patch -p1
###
GCC: 3.1
###
CompileScript: <<
  cat default-fink.conf | xargs ./configure --with-mpm=worker
  make 
  make install DESTDIR=%d
  mv %i/sbin/httpd httpd-worker
  mv %i/share/apache2/httpd.exp httpd.exp-worker
  cd server; make clean
  cat default-fink.conf | xargs ./configure --with-mpm=prefork
  make
  make install DESTDIR=%d
  mv %i/sbin/httpd httpd-prefork
  mv %i/share/apache2/httpd.exp httpd.exp-prefork
  cd server; make clean
  cat default-fink.conf | xargs ./configure --with-mpm=perchild
  make 
  make install DESTDIR=%d
  mv %i/sbin/httpd httpd-perchild
  mv %i/share/apache2/httpd.exp httpd.exp-perchild
  cd server; make clean
  cat default-fink.conf | xargs ./configure --with-mpm=leader
  make 
  make install DESTDIR=%d
  mv %i/sbin/httpd httpd-leader
  mv %i/share/apache2/httpd.exp httpd.exp-leader
  cd server; make clean
  cat default-fink.conf | xargs ./configure --with-mpm=threadpool
  make 
  make install DESTDIR=%d
  mv %i/sbin/httpd httpd-threadpool
  mv %i/share/apache2/httpd.exp httpd.exp-threadpool
  cd server; make clean
  cat default-fink.conf | xargs ./configure
  make
  make install DESTDIR=%d
  mv %i/sbin/httpd httpd-nompm
  mv %i/share/apache2/httpd.exp httpd.exp-nompm
  cd server; make clean
<<
InstallScript: <<
  make install DESTDIR=%d
  rm -rf %i/sbin/httpd
  rm -rf %i/share/apache2/httpd.exp
  install -m 755 httpd-worker %i/sbin
  install -m 755 httpd-prefork %i/sbin
  install -m 755 httpd-perchild %i/sbin
  install -m 755 httpd-leader %i/sbin
  install -m 755 httpd-threadpool %i/sbin
  install -m 755 httpd-nompm %i/sbin
  install -m 644 httpd.exp-worker %i/share/apache2
  install -m 644 httpd.exp-prefork %i/share/apache2
  install -m 644 httpd.exp-perchild %i/share/apache2
  install -m 644 httpd.exp-leader %i/share/apache2
  install -m 644 httpd.exp-threadpool %i/share/apache2
  install -m 644 httpd.exp-nompm %i/share/apache2
  rm -rf %i/etc/apche2/httpd.conf
  install -m 664 httpd.conf.fink %i/etc/apache2/httpd.conf
  rm -rf %i/etc/apche2/httpd-std.conf
  install -m 664 httpd-std.conf.fink %i/etc/apache2/httpd-std.conf
  rm -rf %i/include/apache2/ssl*.h
  rm -rf %i/include/apache2/mod_ssl.h
  rm -rf %i/share/apache2/manual/ssl
  rm -rf %i/share/apache2/manual/mod/*ssl*
<<
