Package: control-center2
Version: 1.99.10
Revision: 1
Source: mirror:gnome:pre-gnome2/sources/control-center/control-center-%v.tar.bz2
Source-MD5: 
Depends: %N-shlibs (= %v-%r)
BuildDepends: 
Patch: %f.patch
SetCPPFLAGS: -no-cpp-precomp
InstallScript: make install DESTDIR=%d
ConfigureParams: --mandir=%p/share/man
SplitOff: <<
	Package: %N-shlibs
	Depends: 
	Files: lib/*.*.dylib
	DocFiles: AUTHORS COPYING* ChangeLog NEWS README
<<
SplitOff2: <<
	Package: %N-dev
	Depends: %N-shlibs (= %v-%r)
	BuildDependsOnly: true
	Files: include lib/pkgconfig lib/*[!.][!.][!.].[adl]*
	DocFiles: AUTHORS COPYING* ChangeLog NEWS README
<<
DocFiles: AUTHORS COPYING* ChangeLog NEWS README
