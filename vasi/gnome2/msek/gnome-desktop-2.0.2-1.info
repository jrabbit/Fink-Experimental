Patch: %f.patch
SetCPPFLAGS: -no-cpp-precomp
NoSetLDFLAGS: true
ConfigureParams: --mandir=%p/share/man
InstallScript: make install DESTDIR=%d
SplitOff: <<
  Package: %N-shlibs
  Depends: gtk+2 (>= 2.0.3), libgnomecanvas2 (>= 2.0.0), gnome-vfs2 (>= 1.9.16) | gnome-vfs2-ssl (>= 1.9.16), libgnomeui2 (>= 1.117.0)
