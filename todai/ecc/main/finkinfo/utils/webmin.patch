diff -Naur ./webmin-1.100/fink-newmods.pl ./webmin-1.100-patched/fink-newmods.pl
--- ./webmin-1.100/fink-newmods.pl	Thu Jan  1 00:00:00 1970
+++ ./webmin-1.100-patched/fink-newmods.pl	Thu Jul 24 22:41:20 2003
@@ -0,0 +1,24 @@
+# newmods.pl
+# Updates an acl file to include new modules. Called with the parameters
+# <config directory> <module>+
+
+for($i=1; $i<@ARGV; $i++) {
+	push(@new, $ARGV[$i]);
+	}
+
+if (@new) {
+	open(ACL, "$ARGV[0]/webmin.acl");
+	@acl = <ACL>;
+	close(ACL);
+	open(ACL, "> $ARGV[0]/webmin.acl");
+	$a = shift(@acl);
+	$a =~ /^(\S+):\s*(.*)$/;
+	$name = $1; @list = split(/\s+/, $2);
+	foreach $o (@list) { $old{$o}++; }
+	foreach $n (@new) {
+		push(@list, $n) if (!$old{$n});
+		}
+	print ACL "$name: ",join(" ",@list),"\n";
+	print ACL @acl;
+	close(ACL);
+	}
diff -Naur ./webmin-1.100/fink-newmodule-postinstall ./webmin-1.100-patched/fink-newmodule-postinstall
--- ./webmin-1.100/fink-newmodule-postinstall	Thu Jan  1 00:00:00 1970
+++ ./webmin-1.100-patched/fink-newmodule-postinstall	Sat Apr 23 14:52:04 2005
@@ -0,0 +1,19 @@
+#!/bin/sh
+# PostInstScript for modules installed via Fink on Mac OS X
+wadir=@PREFIX@/lib/webmin1.0
+config_dir=@PREFIX@/etc/webmin
+os_type=`grep "^os_type=" $config_dir/config | sed -e 's/os_type=//g'`
+os_version=`grep "^os_version=" $config_dir/config | sed -e 's/os_version=//g'`
+case $# in
+    0) # webmin-fink-scripts <= 1.100-2
+    allmods=`cd "$srcdir"; echo */module.info | sed -e 's/\/module.info//g'` ;;
+    *) # webmin-fink-scripts >= 1.100-3
+    allmods=$* ;;
+esac
+#ACL files MUST be updated before the config files
+echo "Updating webmin module acl files..."
+/usr/bin/perl "$wadir/fink-newmods.pl" $config_dir $allmods
+
+echo "Updating webmin module config files..."
+/usr/bin/perl "$wadir/copyconfig.pl" "$os_type" "$os_version" "$wadir" $config_dir "" $allmods
+echo "Done."
