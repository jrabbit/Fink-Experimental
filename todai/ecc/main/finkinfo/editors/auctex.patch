+# 59 Temple Place, Suite 330, Boston, MA 02111-1307, USA.
+
+set -o posix
+
+source @PREFIX@/share/debconf/confmodule
+db_version 2.0
+
+case "${1}" in
+    (configure|reconfigure)
+    if egrep "\brequire[[:space:]]+[^)]*'tex-site\b" \
+	@PREFIX@/etc/emacs/site-start.el 2>/dev/null \
+	| egrep -v ";.*\brequire\b" &>/dev/null; then
+	db_set auctex/default false || true
+	db_text low auctex/alreadydefault || true
+    else
+	db_input medium auctex/default || true
+    fi
+    db_go || true
+    LOGFILE=$(mktemp /tmp/update-auctex-elisp.XXXXXXX)
+    db_register auctex/doauto auctex/logfile || true
+    db_set auctex/logfile ${LOGFILE} || true
+    db_subst auctex/doauto LOGFILE ${LOGFILE} || true
+    db_subst auctex/doautofg LOGFILE ${LOGFILE} || true
+    db_input low auctex/doauto || true
+    db_go || true
+    db_get auctex/doauto || true
+    if [ "${RET}" = "Foreground" ]; then
+			db_input low auctex/doautofg || true
+	db_go || true
+    fi ;;
+    (*) echo >&2 "${0##*/}: Called with unknown argument \`${1}'." ;;
+esac
+
+exit 0
