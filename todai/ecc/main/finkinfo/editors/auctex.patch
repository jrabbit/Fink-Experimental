+# General Public License for more details.
+#
+# You should have received a copy of the GNU General Public License along
+# with this program; if not, write to: The Free Software Foundation, Inc.,
+# 59 Temple Place, Suite 330, Boston, MA 02111-1307, USA.
+
+set -o posix
+
+source @PREFIX@/share/debconf/confmodule
+db_version 2.0
+db_get auctex/logfile || true; _db_logfile=${RET}
+db_get auctex/default || true; _db_default=${RET}
+db_get auctex/doauto || true; _db_doauto=${RET}
+db_get auctex/doautofg || true; _db_doautofg=${RET}
+
+FLAVOR=${1}
+
+INSTALL="install -o root -g admin"
+INSTDIR="${INSTALL} -m 755 -d"
+
+do_install () {
+    echo >&2 -n "install/auctex: Setting up for ${FLAVOR}... "
+    cd @PREFIX@/share/emacs/site-lisp/auctex/; umask 0022;
+    ${INSTDIR} @PREFIX@/share/${FLAVOR}/site-lisp/auctex/
+    ./configure --disable-build-dir-test \
+	--prefix=@PREFIX@/ \
+	--with-emacs=${FLAVOR} \
+	--with-lispdir=@PREFIX@/share/${FLAVOR}/site-lisp/ \
+	--with-auto-dir=@PREFIX@/var/lib/auctex/${FLAVOR} \
+	>> @PREFIX@/share/${FLAVOR}/site-lisp/auctex//CompilationLog 2>&1
+    make INSTALL="${INSTALL}" MKINSTALLDIRS="${INSTDIR}" \
+	install-lisp install-contrib \
+	>> @PREFIX@/share/${FLAVOR}/site-lisp/auctex//CompilationLog 2>&1
+    make distclean \
+	>> @PREFIX@/share/${FLAVOR}/site-lisp/auctex//CompilationLog 2>&1
+    find @PREFIX@/share/${FLAVOR}/site-lisp/auctex/ -type f -name \*.el \
+	-print0 | xargs --null rm -f dummy \
+	>> @PREFIX@/share/${FLAVOR}/site-lisp/auctex//CompilationLog 2>&1
+    gzip -9fq @PREFIX@/share/${FLAVOR}/site-lisp/auctex//CompilationLog
+    echo -e >@PREFIX@/etc/${FLAVOR}/site-start.d//50auctex.el \
+	";;; This file is automatically generated.\n"
+    echo -e >>@PREFIX@/etc/${FLAVOR}/site-start.d//50auctex.el \
+	"(if (and nil (fboundp 'fink-pkg-add-load-path-item))\n" \
+	"   (progn (fink-pkg-add-load-path-item \"@PREFIX@/share/${FLAVOR}/site-lisp/auctex/\")\n" \
+	"          (fink-pkg-add-load-path-item \"@PREFIX@/share/emacs/site-lisp/auctex/\"))\n" \
+	" (progn (add-to-list 'load-path \"@PREFIX@/share/${FLAVOR}/site-lisp/auctex/\" 'append)\n" \
+	"        (add-to-list 'load-path \"@PREFIX@/share/emacs/site-lisp/auctex/\" 'append)))"
+    if [ "${_db_default}" = "true" ]; then
+	echo -e >>@PREFIX@/etc/${FLAVOR}/site-start.d//50auctex.el \
+	"\n(require 'tex-site)"
+    fi
+    echo >&2 "done."
+    return 0
+}
+
+do_auto () {
+    case "${_db_doauto}" in
+	(Foreground)
+	case "${_db_doautofg}" in
+	    (Console)
+	    rm -f ${_db_logfile}
+	    @PREFIX@/sbin//update-auctex-elisp ${FLAVOR} ;;
+	    (File)
+	    echo >&2 -n "update-auctex-elisp: "
+	    echo >&2 "Further output will appear in: ${_db_logfile}."
+	    echo >&2 -n "auctex: "
+	    echo >&2 -n "Waiting for update-auctex-elisp to terminate... "
+	    @PREFIX@/sbin//update-auctex-elisp ${FLAVOR} >> ${_db_logfile} 2>&1
+	    echo >&2 "done." ;;
+	    (*) echo >&2 \
+		"${0##*/}: Debconf passed unknown value \`${_db_doautofg}'." ;;
+	esac ;;
+	(Background)
+	# Closing fd 3 is debconf important.
+						@PREFIX@/sbin//update-auctex-elisp ${FLAVOR} >> ${_db_logfile} 2>&1 3>&- &
+	echo >&2 -n "update-auctex-elisp[${!}]: "
+	echo >&2 "Further output will appear in: ${_db_logfile}." ;;
+	(None) rm -f ${_db_logfile} ;;
+	(*) echo >&2 \
+	    "${0##*/}: Debconf passed unknown value \`${_db_doauto}'." ;;
+    esac
+    return 0
+}
+
+case "${FLAVOR}" in
+    (emacs) : ;;
+    (emacs21|emacs21-carbon)
+        do_install ${FLAVOR}
+        do_auto ${FLAVOR} ;;
+    (*) echo >&2 "install/auctex: Ignoring emacsen flavor ${FLAVOR}." ;;
+esac
+
+exit 0
