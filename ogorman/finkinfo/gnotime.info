Package: gnotime
Version: 2.2.0
Revision: 10
Maintainer: Peter O'Gorman <peter@pogma.com>
Source: mirror:sourceforge:gttr/%n-%v.tar.gz
Source-MD5: d1a56daf932c4334f92572335ba9b7a3
ConfigureParams: --disable-dependency-tracking --disable-schemas-install --with-gconf-schema-file-dir=%i/etc/gconf/gconf.xml.defaults/schemas/apps/%n/Display/
NoSetLDFLAGS: true
NoSetCPPFLAGS: true
SetCPPFLAGS: -I%p/include/libgnomeprint-2.2 -I%p/include/libgtkhtml-3.0/
SetLIBS: -lX11 -L/usr/X11R6/lib
BuildDepends: atk1, audiofile, esound, gail17-dev, gal199-dev, gconf2-dev, gettext-dev, glib2-dev, gtk+2-dev, gtkhtml3-dev, guile16-dev, libart2, libbonobo2-dev, libbonoboui2-dev, libglade2, libgnome2-dev, libgnomecanvas2-dev, libgnomeprint2.2-dev, libgnomeprintui2.2-dev, libgnomeui2-dev, libiconv-dev, libjpeg, libxml2, orbit2-dev, pango1-xft2-dev, popt, x11-dev 
Depends: atk1-shlibs, audiofile-shlibs, esound-shlibs, gail17-shlibs, gal199-shlibs, gconf2-shlibs, glib2-shlibs, gnome-vfs2-ssl-shlibs, gtk+2-shlibs, gtkhtml3-shlibs, guile16-shlibs, libart2-shlibs, libbonobo2-shlibs, libbonoboui2-shlibs, libglade2-shlibs, libgnome2-shlibs, libgnomecanvas2-shlibs, libgnomeprint2.2-shlibs, libgnomeprintui2.2-shlibs, libgnomeui2-shlibs, libiconv, libjpeg-shlibs, libxml2-shlibs, orbit2-shlibs, pango1-xft2-shlibs, popt-shlibs, scrollkeeper, x11-shlibs
Patch: %n.patch
CompileScript: <<
#! /bin/sh -ex
export LIBRARY_PATH=%p/lib
export CPATH=%p/include
./configure %c
echo "all:" > gconf/Makefile
echo "install:" >> gconf/Makefile
make includedir="" || make
<<
InstallScript: <<
#! /bin/sh -ex
make install DESTDIR=%d
<<
PostInstScript: <<
scrollkeeper-update -o %p/share/omf/gnotime
<<
